<%= link_to "New User", new_admin_user_path %>
<br>
<% @users.each do |user| %>
<%= link_to user.email, admin_users_path  %>
<%= user.full_name %>
<%= user.admin %>
<%= link_to "edit", edit_admin_user_path(user) %>
<%= link_to "delete", admin_user_path(user), method: 'delete', data: { confirm: "You sure?" } %>
<p></p>
<% end %>

<%= paginate @users %>
